#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
display_board(board)
puts "Where would you like to go?"
input = gets.strip
def input_to_index(index)
index = input_to_index(input)
end
move(board,index, "X")
display_board(board)
