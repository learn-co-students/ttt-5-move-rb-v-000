#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " "," "]
puts "Where would you like to go?"
board_input = gets.strip
board_index = input_to_index(board_input)
move(board,board_index)
display_board(board)
