#!/usr/bin/env ruby

require_relative '../lib/move.rb'

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"# Code your CLI Here
puts "Where do you want to go?"
input = gets.strip
index = input_to_index("#{input}")
move(board, index, value = "X")
display_board(board)
