#!/usr/bin/env ruby

require_relative '../lib/move.rb'
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Welcome to Tic Tac Toe!"# Code your CLI Here
puts "Please enter a number from 1-9"
input = gets.strip
index = input_to_index(input)
move(board, index, character="X")
display_board(board)
